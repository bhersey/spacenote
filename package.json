{
  "name": "spacenote_express",
  "version": "1.0.0",
  "description": "server component of spacenote site",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets=env",
    "client": "cd client && yarn start",
    "server": "nodemon --exec npm run babel-node -- ./server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhersey/spacenote.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bhersey/spacenote/issues"
  },
  "homepage": "https://github.com/bhersey/spacenote#readme",
  "dependencies": {
    "apollo-fetch": "^0.7.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.1",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.17.1",
    "chokidar": "^2.0.0",
    "cors": "^2.8.3",
    "express": "^4.16.2",
    "graphql": "^0.12.3",
    "graphql-server-express": "^1.3.2",
    "graphql-tools": "^2.19.0",
    "morgan": "^1.9.0",
    "musicmetadata": "^2.0.5",
    "neo4j-driver": "^1.2.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
